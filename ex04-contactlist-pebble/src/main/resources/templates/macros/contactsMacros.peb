    {% macro listRow(label, list) %}
    <tr>
        <td>
            <ul>
                    {% for e in list %}
                    <li> {{ e }}</li>
                    {% endfor %}
            </ul>
        </td>
    </tr>
    {% endmacro %}

    {% macro simpleRow(label, value) %}
    <tr>
        <td>{{ label }}</td>
        <td>{{  value }}</td>
    </tr>
    {% endmacro %}
